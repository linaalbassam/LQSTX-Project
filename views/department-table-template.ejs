<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

<table class="table table-bordered">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Date</th>
            <th scope="col">Total Worked</th>
            <th scope="col">Billable</th>
            <th scope="col">Non Billable</th>
            <th scope="col">Total on WO</th>
            <th scope="col">Percent Billable</th>
            <th scope="col">Percent on WO</th>
        </tr>
    </thead>
    <tbody>
        <% for (var item = 0; item < data.length; item++) { %>
            <tr>
                <td scope="row"><%=[item+1]%> </td>
                <td><%=moment(data[item].date).add(1,'d').format('MM-DD-YYYY')%></td> 
                <td><%=data[item].total_worked%></td>
                <td><%=data[item].billable%></td>
                <td><%=data[item].non_billable%></td>
                <td><%=data[item].total_on_WO%></td>
                <td><%=data[item].percent_billable%></td>
                <td><%=data[item].percent_on_WO%></td>
            </tr>    
        <% } %>  
    </tbody>
</table>
